{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "Debug",
            "url": "http://localhost:5000",
            "webRoot": "${workspaceRoot}",
            "sourceMaps": true,
            "sourceMapPathOverrides": {
                "webpack:///*": "${webRoot}/*"
            },
            "userDataDir": "${workspaceRoot}/.vscode/chrome"
        }
    ]
}